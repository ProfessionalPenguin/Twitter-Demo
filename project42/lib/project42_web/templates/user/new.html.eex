
<h1>New User</h1>

<%= form_for @conn, Routes.user_path(@conn, :create), fn f -> %>
  <div>
    <%= text_input f, :username, placeholder: "Username" %>
  </div>
    <div>
    <%= text_input f, :password, placeholder: "password" %>
  </div>
   <div>
    <%= hidden_input f, :id , value: Enum.random(100..1000) %>
    </div>
  <%= submit "Create User" %>
<% end %>
